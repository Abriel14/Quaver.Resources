<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="SFX\**\*" />
    <EmbeddedResource Include="Textures\**\*" />
    <EmbeddedResource Include="Fonts\**\*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="QuaverResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>QuaverResources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="QuaverResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>QuaverResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>
